	<queryString language="SQL">
		<![CDATA[SELECT colegio.nome, colegio.tipo,
	COUNT(colegio.codigo)		
FROM kit_alimentacao	
	INNER JOIN colegio ON 
	 kit_alimentacao.colegio = colegio.codigo 	
WHERE 
	 kit_alimentacao.ativo = 'true' 	
GROUP BY colegio.nome,colegio.tipo
ORDER BY colegio.tipo]]>
