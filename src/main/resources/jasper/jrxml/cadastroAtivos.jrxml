	<queryString language="SQL">
		<![CDATA[SELECT kit_alimentacao.nome_aluno,
	kit_alimentacao.nome_responsavel,
	kit_alimentacao.ativo,
	kit_alimentacao.cpf,
	kit_alimentacao.turma,
	kit_alimentacao.turno,
	colegio.nome,
	serie_colegio.nome
FROM kit_alimentacao	
	INNER JOIN colegio ON 
	 kit_alimentacao.colegio = colegio.codigo 
	INNER JOIN serie_colegio ON 
	 kit_alimentacao.serie_colegio = serie_colegio.codigo 
	 AND ( 
	 $P{ESCOLA} IS NULL  
	 OR colegio.codigo = $P{ESCOLA}) 
	 AND ( 
	 $P{SERIE} IS NULL  
	 OR serie_colegio.codigo = $P{SERIE}) 
	 AND ( 
	 $P{TURMA} IS NULL  
	 OR kit_alimentacao.turma LIKE $P{TURMA}) 
	 AND ( 
	 $P{TURNO} IS NULL  
	 OR kit_alimentacao.turno LIKE $P{TURNO}) 
	 AND kit_alimentacao.ativo = 'true']]>
	</queryString>
