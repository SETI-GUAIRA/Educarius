	<queryString language="SQL">
		<![CDATA[SELECT kit_alimentacao.nome_aluno,
	kit_alimentacao.nome_responsavel,
	kit_alimentacao.ativo,
	kit_alimentacao.cpf,
	kit_alimentacao.turma,
	kit_alimentacao.turno,	
	colegio.nome,
	serie_colegio.nome
FROM pedido_Item
	INNER JOIN kit_alimentacao ON 
	 pedido_Item.codigo_kit_alimentacao = kit_alimentacao.codigo 
	INNER JOIN colegio ON 
	 kit_alimentacao.colegio = colegio.codigo 
	INNER JOIN serie_colegio ON 
	 kit_alimentacao.serie_colegio = serie_colegio.codigo]]>
	</queryString>
